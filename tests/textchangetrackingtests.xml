  <test name="singleinsertiontext_office_text">
    <input type="odt1.2">
      <o:text>
        <text:tracked-changes text:track-changes="false">
          <text:changed-region text:id="otit" xml:id="otit">
            <text:insertion>
              <o:change-info>
                <dc:creator>Test</dc:creator>
                <dc:date>2014-12-09T12:33:00</dc:date>
                <text:p>Comment 1</text:p>
                <text:p>Comment 2</text:p>
              </o:change-info>
            </text:insertion>
          </text:changed-region>
        </text:tracked-changes>
        <text:p><text:change-start text:change-id="otit"/>Hello<text:change-end text:change-id="otit"/></text:p>
      </o:text>
    </input>
    <output types="odt1.2 odt1.2ext">
      <file path="content.xml">
        <xpath expr="count(//o:text/text:tracked-changes/text:changed-region[@text:id or @id]/text:insertion)=1"/>
        <xpath expr="//o:text/text:tracked-changes/text:changed-region/text:insertion/o:change-info/dc:creator='Test'"/>
        <xpath expr="//o:text/text:tracked-changes/text:changed-region/text:insertion/o:change-info/dc:date='2014-12-09T12:33:00'"/>
        <xpath expr="count(//o:text/text:tracked-changes/text:changed-region/text:insertion/o:change-info/text:p)=2"/>
        <xpath expr="//o:text/text:tracked-changes/text:changed-region/text:insertion/o:change-info/text:p[1]='Comment 1'"/>
        <xpath expr="//o:text/text:tracked-changes/text:changed-region/text:insertion/o:change-info/text:p[2]='Comment 2'"/>
        <xpath expr="count(//o:text/text:p)=1"/>
        <xpath expr="count(//o:text/text:p/text:change-start/@text:change-id)=1"/>
        <xpath expr="(//o:text/text:p/text:change-start/@text:change-id = //o:text/text:p/text:change-end/@text:change-id)"/>
        <xpath expr="count(//o:text/text:p/text:change-end[preceding::text:change-start])=1"/>
        <xpath expr="(//o:text/text:tracked-changes/text:changed-region/@text:id = //o:text/text:p/text:change-start//@text:change-id) or (//o:text/text:tracked-changes/text:changed-region/@id = //o:text/text:p/text:change-start//@text:change-id)"/>
      </file>
    </output>
    <pdf/>
  </test>
